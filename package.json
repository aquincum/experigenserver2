{
  "name": "experigenserver2",
  "version": "2.0.0",
  "description": "Experigen server rethought using MongoDB",
  "main": ".#main.js",
  "scripts": {
    "postinstall": "node_modules/bower/bin/bower install -F --allow-root",
    "start": "node main.js",
    "jsdoc": "node ./node_modules/jsdoc/jsdoc.js -r -R README.md -c jsdoc.conf.json -t ./node_modules/ink-docstrap/template -d doc main.js server/",
    "test": "./node_modules/mocha/bin/mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aquincum/experigenserver2.git"
  },
  "keywords": [
    "experiment",
    "server"
  ],
  "author": "Daniel Szeredi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aquincum/experigenserver2/issues"
  },
  "homepage": "https://github.com/aquincum/experigenserver2#readme",
  "dependencies": {
    "bower": "*",
    "codecov": ">1.0.0",
    "express": "*",
    "express-session": "*",
    "fnv-plus": "*",
    "istanbul": ">0.3.2",
    "mocha": "*",
    "mongodb": ">=2.0",
    "morgan": "^1.6.1",
    "passport": "*",
    "passport-anonymous": "*",
    "passport-http": "*",
    "yargs": "*"
  },
  "devDependencies": {
    "blanket": "^1.1.9",
    "coveralls": "^2.11.4",
    "ink-docstrap": "^1.1.0",
    "jasmine-core": "^2.4.1",
    "jsdoc": "^3.3.3",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.3",
    "mocha-lcov-reporter": "^1.0.0",
    "supertest": "^1.1.0"
  },
  "config": {
    "blanket": {
      "pattern": [
        ""
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  }
}
